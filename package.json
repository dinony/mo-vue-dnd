{
  "name": "mo-vue-dnd",
  "version": "0.4.2",
  "description": "Drag and drop using just Vue.js",
  "main": "dist/mo-vue-dnd.cjs.js",
  "unpkg": "dist/mo-vue-dnd.umd.js",
  "module": "dist/mo-vue-dnd.esm.js",
  "style": "dist/mo-vue-dnd.css",
  "scripts": {
    "lint": "eslint src/**/*.js",
    "lint:demos": "eslint demo/src/*.js",
    "build:lib": "concurrently \"rollup -c rollup.config.lib.js --environment NODE_ENV:production\" \"gulp scss:lib\"",
    "release": "npm run build:lib && npm publish",
    "css:lib": "sass --watch src/index.scss:dist/mo-vue-dnd.css",
    "css:libdemo": "sass --watch src/index.scss:demo/dist/mo-vue-dnd.css",
    "build:demo": "rollup -c rollup.config.demo.js --environment NODE_ENV:production && gulp scss:demo && gulp scss:libdemo",
    "js:demo": "rollup -w -c rollup.config.demo.js",
    "css:demo": "sass --watch demo/src/index.scss:demo/dist/index.css",
    "dev:demo": "concurrently --kill-others-on-fail \"npm run js:demo\" \"npm run css:demo\" \"npm run css:libdemo\" \"npm run serve:demos\"",
    "deploy:demo": "npm run build:demo && tools/deploy.sh",
    "serve:demos": "node tools/serve.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "Drag and drop"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dinony/mo-vue-dnd.git"
  },
  "author": "Onur Dogangönül <contact@dinony.com>",
  "license": "MIT",
  "browserslist": [
    "> 1%"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.5.1",
    "browser-sync": "^2.23.6",
    "concurrently": "^3.5.1",
    "cssnano": "^3.10.0",
    "eslint": "^4.17.0",
    "gulp": "^3.9.1",
    "gulp-postcss": "^7.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "node-sass": "^4.7.2",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-uglify": "^3.0.0",
    "vue": "^2.5.13"
  }
}
